{
  "manifest_version": 2,

  "name": "Ysod Helper",
  "description": "This extension alters the YSOD page to include links",
  "version": "1.0",

  
  "content_scripts": [
	{
		"matches": ["http://*/*"],
		"js":["ysod.js"],
		"run_at" : "document_end"
	}
  ],
  "permissions": [ 
	"<all_urls>",
	"cookies",
	"http://*/*",
    "https://*/*",
	"file:///*",
	"tabs"
  ]
}
